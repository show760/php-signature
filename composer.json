{
    "name": "show760/php-signature",
    "description": "This is a library which help you to generate signature",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=7.0",
        "ext-json": "*"
    },
    "autoload": {
        "psr-4": {
            "Token\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Token\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^6 || ^8",
        "squizlabs/php_codesniffer": "^3.6"
    }
}
